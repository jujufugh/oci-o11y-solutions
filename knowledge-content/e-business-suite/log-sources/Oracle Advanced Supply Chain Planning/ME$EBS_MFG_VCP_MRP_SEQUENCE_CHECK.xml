    <DisplayName>EBS Sensor: MFG VCP MRP Sequence Check detected sequence in need of maintenance - Weekly</DisplayName>
    <Description><![CDATA[Sequences reported by this SQL must have maintenance conducted to avoid errors in the future.]]></Description>
    <IsSystem>0</IsSystem>
    <IsSecureContent>1</IsSecureContent>
    <Patterns>
        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'ME$EBS_MFG_VCP_MRP_SEQUENCE_CHECK' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, 'OWNER:' || s.OWNER || ', SEQUENCE_NAME:' || s.SEQUENCE_NAME || ', MAX_VALUE_ALLOWED:' || s.MAX_VALUE_ALLOWED || ', CURRENT_VALUE:' || s.CURRENT_VALUE || ', NUMBER_OF_VALUES_AVAILABLE:' || s.NUMBER_OF_VALUES_AVAILABLE key_data FROM (SELECT OWNER OWNER,  SEQUENCE_NAME SEQUENCE_NAME,  MAX_VALUE_ALLOWED MAX_VALUE_ALLOWED,  CURRENT_VALUE CURRENT_VALUE,  NUMBER_OF_VALUES_AVAILABLE NUMBER_OF_VALUES_AVAILABLE  from  (SELECT sequence_owner OWNER,  sequence_name SEQUENCE_NAME,  max_value MAX_VALUE_ALLOWED,  last_number CURRENT_VALUE,  (MAX_VALUE - LAST_NUMBER) NUMBER_OF_VALUES_AVAILABLE  FROM DBA_SEQUENCES  WHERE sequence_name IN ('MRP_GROSS_REQUIREMENTS_S','MRP_SCHEDULE_DATES_S','MRP_FULL_PEGGING_S', 'MRP_FORECAST_DATES_S','MRP_AVAILABLE_TO_PROMISE_S','MRP_EXCEPTION_DETAILS_S', 'CRP_RESOURCE_PLAN_S','CRP_RESOURCE_HOURS_S')  AND last_number > 1947483647)) s]]>
        </SQLQuery>
