    <DisplayName>EBS Sensor: FIN GL Incorrect Program Incompatibilities - Weekly</DisplayName>
    <Description><![CDATA[Incorrect Program Incompatibilities found for General ledger Concurrent programs.]]></Description>
    <IsSystem>0</IsSystem>
    <IsSecureContent>1</IsSecureContent>
    <Patterns>
        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'ME$EBS_FIN_GL_CONCURRENT_PROGRAM_INCOMPATIBILITIES' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, 'program_name:' || s.program_name key_data FROM (SELECT distinct fc1.user_concurrent_program_name program_name  FROM apps.FND_CONCURRENT_PROGRAM_SERIAL FCP ,  apps.FND_CONCURRENT_PROGRAMS_VL FC1 ,  apps.FND_CONCURRENT_PROGRAMS_VL FC2  WHERE FC1.application_id = 101  AND FCP.to_run_concurrent_program_id = FC1.concurrent_program_id  AND FCP.incompatibility_type = 'D'  AND FC1.cd_parameter IS NULL  AND FC2.application_id = FCP.running_application_id  AND FC2.concurrent_program_id = FCP.running_concurrent_program_id  AND FC2.application_id = 101  and rownum<1000) s]]>
        </SQLQuery>
