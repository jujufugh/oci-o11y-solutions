    <DisplayName>EBS Sensor: FIN GL Duplicate Segment Values Assigned to the Ledger - Weekly</DisplayName>
    <Description><![CDATA[Rows returned by the query indicate duplicate assignments.  A data fix may be required to remove the duplicate records.]]></Description>
    <IsSystem>0</IsSystem>
    <IsSecureContent>1</IsSecureContent>
    <Patterns>
        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'ME$EBS_FIN_GL_GL_DUPLICATE_SEGMENT_VALUES_V2' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, 'segment_value:' || s.segment_value || ', duplicate_segment_values:' || s.duplicate_segment_values || ', ledger_name:' || s.ledger_name key_data FROM (SELECT  ledger_name ledger_name,  segment_value segment_value,  duplicate_segment_values duplicate_segment_values from (  (SELECT  gl.name ledger_name,  gnv.segment_value,  COUNT(1) duplicate_segment_values  FROM  apps.gl_ledger_segment_values gnv,  apps.gl_ledgers gl  WHERE  gnv.ledger_id = gl.ledger_id  GROUP BY  gl.name,  gnv.segment_value  HAVING  COUNT(1) > 1)  Union  (SELECT  gl.name ledger_name,  gnvs.segment_value,  COUNT(1) duplicate_segment_values  FROM  apps.gl_ledger_norm_seg_vals gnvs,  apps.gl_ledgers gl  WHERE  gnvs.ledger_id = gl.ledger_id  GROUP BY  gl.name,  gnvs.segment_value  HAVING  COUNT(1) > 1))  where ROWNUM < = 10) s]]>
        </SQLQuery>
        <IsInclude>1</IsInclude>
            <InUse>1</InUse>
