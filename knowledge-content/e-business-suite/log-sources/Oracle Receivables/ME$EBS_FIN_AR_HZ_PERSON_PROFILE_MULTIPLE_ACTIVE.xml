    <DisplayName>EBS Sensor: FIN AR Multiple Active Person Profile - Weekly</DisplayName>
    <Description><![CDATA[HZ_PERSON_PROFILES has multiple records for a PARTY_ID where EFFECTIVE_END_DATE is NULL.]]></Description>
    <IsSystem>0</IsSystem>
    <IsSecureContent>1</IsSecureContent>
    <Patterns>
        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'ME$EBS_FIN_AR_HZ_PERSON_PROFILE_MULTIPLE_ACTIVE' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, 'PERSON:' || s.PERSON || ', PARTY_ID:' || s.PARTY_ID key_data FROM (select hpp.PARTY_ID PARTY_ID,          hpp.PERSON_NAME PERSON    from apps.HZ_PARTIES hp,          apps.HZ_PERSON_PROFILES hpp    where hp.PARTY_ID = hpp.PARTY_ID      and hpp.EFFECTIVE_END_DATE is null     and rownum <= 5   group by hpp.PARTY_ID, hpp.PERSON_NAME  having count(*) > 1) s]]>
